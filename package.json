{
  "name": "get-osx-version",
  "version": "3.0.0",
  "description": "tiny program to get the Mac OS version of the current system",
  "main": "index.js",
  "scripts": {
    "test": "nyc node index.test.js",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fscherwi/get-osx-version.git"
  },
  "keywords": [
    "osx",
    "mac",
    "version",
    "mac os",
    "macos"
  ],
  "author": "fscherwi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fscherwi/get-osx-version/issues"
  },
  "homepage": "https://github.com/fscherwi/get-osx-version#readme",
  "bin": {
    "get-osx-version": "bin.js"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint-plugin-jsdoc": "^18.1.4",
    "nyc": "^14.1.1",
    "xo": "^0.25.3"
  },
  "files": [
    "bin.js",
    "index.js"
  ],
  "nyc": {
    "reporter": [
      "lcov"
    ]
  },
  "xo": {
    "rules": {
      "unicorn/no-process-exit": 0,
      "max-params": 0
    },
    "plugins": [
      "jsdoc"
    ],
    "extends": [
      "plugin:jsdoc/recommended"
    ]
  }
}
